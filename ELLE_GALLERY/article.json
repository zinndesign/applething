								{
									"role": "image",
									"URL": "http://ell.h-cdn.co/assets/15/28/gettyimages-103368251_master.jpg",
									"layout": "photoLayout",
									"animation": {
										"type": "fade_in",
										"userControllable": true,
										"initialAlpha": 0.1
