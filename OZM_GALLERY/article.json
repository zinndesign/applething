	"components": [
		{
			"role": "header",
			"layout": "headerImageLayout",
			"style": {
				"fill": {
					"type": "image",
					"URL": "http://doz.h-cdn.co/assets/15/38/1600x800/landscape-1442618576-depression-intro.jpg",
					"fillMode": "cover",
					"verticalAlignment": "top"
				}
